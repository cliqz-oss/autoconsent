{
  "name": "Sourcepoint",
  "detectCmp": [{ "visible": "div[id^='sp_message_panel_'" }],
  "detectPopup": [{ "visible": "div[id^='sp_message_panel_'" }],
  "frame": "https://pm.sourcepoint.mgr.consensu.org",
  "optOut": [
    {
      "click": "button[class^='sp_choice']",
      "waitForFrame": true
    },
    {
      "waitFor": ".priv-vendor-block",
      "frame": true
    },
    {
      "click": "a[id^='tab-pan_'] > .right",
      "all": true,
      "frame": true,
      "optional": true
    },
    {
      "click": ".on > .inner-switch",
      "all": true,
      "frame": true,
      "optional": true
    },
    {
      "click": "#tab-saveandexit",
      "frame": true
    }
  ],
  "optIn": [
    {
      "click": "button[class^='sp_choice']",
      "waitForFrame": true
    },
    {
      "waitFor": ".priv-vendor-block",
      "frame": true
    },
    {
      "click": "a[id^='tab-pan_'] > .left",
      "all": true,
      "frame": true,
      "optional": true
    },
    {
      "click": ".switch-bg:not(.on) > .inner-switch",
      "all": true,
      "frame": true,
      "optional": true
    },
    {
      "click": "#tab-saveandexit",
      "frame": true
    }
  ]
}